--[[m1 effect]]
-- Animation IDs to check for
local TriggerAnimations = {
    "rbxassetid://15259161390",
    "rbxassetid://15240216931",
    "rbxassetid://15240176873",
    "rbxassetid://15162694192"
}

-- Reference to the effect
local Test = game.ReplicatedStorage.Resources.CrabBeam.Beamy["End"].Attachment

-- Get the local player and humanoid
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

-- Function to check if an animation matches the trigger list
local function isTriggerAnimation(animationTrack)
    for _, id in ipairs(TriggerAnimations) do
        if animationTrack.Animation.AnimationId == id then
            return true
        end
    end
    return false
end

-- Function to handle the effect
local function triggerEffect()
    local test = Test:Clone()
    test.Parent = character:WaitForChild("HumanoidRootPart")

    -- Loop through all children to enable the particle emitters and change color to white
    for _, child in ipairs(test:GetChildren()) do
        if child:IsA("ParticleEmitter") then
            -- Set the particle color to white
            child.Color = ColorSequence.new(Color3.fromRGB(255, 255, 255))  -- White color

            -- Ensure particles are fully visible
            child.LightEmission = 1  -- Ensure light emission is on (optional)
            child.Transparency = NumberSequence.new(0)  -- Set transparency to 0 (fully visible)

            -- Emit particles
            child:Emit(15)
            child.Enabled = true  -- Enable particles
        end
    end

    -- Remove the effect after 1 second
    task.delay(0.5, function()
        test:Destroy()
    end)
end

-- Connect to animation played event
humanoid.AnimationPlayed:Connect(function(animationTrack)
    if isTriggerAnimation(animationTrack) then
        triggerEffect()
    end
end)
